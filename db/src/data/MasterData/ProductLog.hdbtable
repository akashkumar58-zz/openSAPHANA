COLUMN TABLE "MD.productLog" (
     "PRODUCTID" NVARCHAR(10) NOT NULL COMMENT 'Product ID',
	 "LOGID" INTEGER CS_INT NOT NULL COMMENT 'Change Log Entry ID',
	 "DATETIME" SECONDDATE CS_SECONDDATE NOT NULL COMMENT 'Change Date and Time',
	 "USERNAME" NVARCHAR(80) NOT NULL COMMENT 'Changed By',
	 "LOGTEXT" NVARCHAR(500) COMMENT 'Chagne Log Details',
	 PRIMARY KEY ("PRODUCTID", "LOGID", "DATETIME", "USERNAME")) 
	 COMMENT 'Product Change Log'
	 UNLOAD PRIORITY 5 AUTO MERGE 