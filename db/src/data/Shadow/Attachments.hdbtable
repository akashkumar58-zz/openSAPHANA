 COLUMN TABLE "shadow::UtilShadow.Attachments" (
     "NOTEID" NVARCHAR(10) NOT NULL COMMENT 'Note ID',
	 "LINKID" NVARCHAR(10) COMMENT 'Link ID',
	 "TYPE" NVARCHAR(30) COMMENT 'Attachment Type',
	 "TITLE" NVARCHAR(255) COMMENT 'Title',
	 "CREATEDAT" DATE CS_DAYDATE COMMENT 'Created Date',
	 "DESCRIPTION" NVARCHAR(255) COMMENT 'Description Text',
	 "SIZE" NVARCHAR(40) COMMENT 'Size',
	 PRIMARY KEY ("NOTEID")) 
	 COMMENT 'Attachments'
	 UNLOAD PRIORITY 5 AUTO MERGE 