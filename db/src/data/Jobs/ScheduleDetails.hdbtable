COLUMN TABLE "Jobs.ScheduleDetails" (
     "JOBID" NVARCHAR(10) NOT NULL COMMENT 'Job ID',
	 "NAME" VARCHAR(40) COMMENT 'Schedule Name',
	 "STARTTIME" LONGDATE CS_LONGDATE COMMENT 'Start Time',
	 "ENDTIME" LONGDATE CS_LONGDATE COMMENT 'End Time',
	 "CRON" VARCHAR(40) COMMENT 'CRON String',
	 "SCHEDULE" VARCHAR(40) COMMENT 'Schedule',
	 PRIMARY KEY ("JOBID")) 
	 COMMENT 'Job Schedules'
	 UNLOAD PRIORITY 5 AUTO MERGE 